            <TextArea fx:id="resultTextArea" editable="false" layoutX="100.0" layoutY="55.0" prefHeight="112.0" prefWidth="200.0" />
            <Button fx:id="OKButton" layoutX="74.0" layoutY="308.0" mnemonicParsing="false" onAction="#OKButtonClick" text="OK" />
            <Button fx:id="CancelButton" layoutX="274.0" layoutY="308.0" mnemonicParsing="false" onAction="#CancelButtonClick" text="Cancel" />
